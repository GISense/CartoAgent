{
    "version": 8,
    "name": "Neighborhood",
    "metadata": {
        "mapbox:print": {
            "width": 1024,
            "height": 768,
            "units": "px",
            "format": "png",
            "resolution": 300
        }
    },
    "center": [121.485, 31.236],
    "zoom": 14.2,
    "sources": {
        "composite": {
            "url": "mapbox://mapbox.mapbox-streets-v8",
            "type": "vector"
        }
    },
    "sprite": "mapbox://sprites/{username}/{style_id}",
    "glyphs": "mapbox://fonts/{username}/{fontstack}/{range}.pbf",
    "layers": [
        {
            "id": "Background",
            "type": "background",
            "paint": {
                "background-color": "#f5f4ef"
            }
        },
        {
            "id": "Commercial_area",
            "type": "fill",
            "source": "composite",
            "source-layer": "landuse",
            "paint":{
                "fill-opacity": [
                    "match",
                    ["get","class"],
                    ["commercial_area"],
                    1,
                    0
                ],
                "fill-color": "#f0e7cc",
                "fill-outline-color": "#f9e401"
            }
        },
        {
            "id": "Green_space",
            "type": "fill",
            "source": "composite",
            "source-layer": "landuse",
            "paint":{
                "fill-opacity": [
                    "match",
                    ["get","class"],
                    ["park","wood","grass"],
                    1,
                    0
                ],
                "fill-color": "#e6e7d5",
                "fill-outline-color": "#cfded2"
            }
        },
        {
            "id": "Water",
            "type": "fill",
            "source": "composite",
            "source-layer": "water",
            "paint": {
                "fill-opacity": 1,
                "fill-color": "#c1ccd7",
                "fill-outline-color": "#bfcdd4"
            }
        },
        {
            "id": "Commercial_building",
            "type": "fill",
            "source": "composite",
            "source-layer": "building",
            "paint": {
                "fill-opacity": [
                    "match",
                    ["get", "type"],
                    ["retail", "commercial", "hotel"],
                    1,
                    0
                ],
                "fill-color": "#efdce2",
                "fill-outline-color": "#ebdbdb"
            }
        },
        {
            "id": "Residential_building",
            "type": "fill",
            "source": "composite",
            "source-layer": "building",
            "paint": {
                "fill-opacity": [
                    "match",
                    ["get", "type"],
                    ["building", "apartments", "hut"],
                    1,
                    0
                ],
                "fill-color": "#e9e2e8",
                "fill-outline-color": "#f1e2f3"
            }
        },
        {
            "id": "Walkway",
            "type": "line",
            "source": "composite",
            "source-layer": "road",
            "paint": {
                "line-opacity": [
                    "match",
                    ["get", "class"],
                    ["pedestrian"],
                    1,
                    0
                ],
                "line-color": "#dcddc5",
                "line-width": 1.5
            }
        },
        {
            "id": "Street",
            "type": "line",
            "source": "composite",
            "source-layer": "road",
            "paint": {
                "line-opacity": [
                    "match",
                    ["get", "class"],
                    ["street"],
                    1,
                    0
                ],
                "line-color": "#e6dbc7",
                "line-width": 1.5
            }
        },
        {
            "id": "Tertiary_road",
            "type": "line",
            "source": "composite",
            "source-layer": "road",
            "paint": {
                "line-opacity": [
                    "match",
                    ["get", "class"],
                    ["tertiary"],
                    1,
                    0
                ],
                "line-color": "#e6ddcb",
                "line-width": 2.5
            }
        },
        {
            "id": "Secondary_road",
            "type": "line",
            "source": "composite",
            "source-layer": "road",
            "paint": {
                "line-opacity": [
                    "match",
                    ["get", "class"],
                    ["secondary"],
                    1,
                    0
                ],
                "line-color": "#e6ddcb",
